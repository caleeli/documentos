{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:caleeli/firmas.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:caleeli/jdd-forms.git"
        },
        {
            "type": "path",
            "url": "../SchemaLive"
        },
        {
            "type": "path",
            "url": "../IsolatedExpression"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "anhskohbo/no-captcha": "^3.0",
        "barryvdh/laravel-dompdf": "^0.8.4",
        "davidc/schema-live": "*",
        "doctrine/dbal": "^2.8",
        "fideloper/proxy": "^4.0",
        "jdd/forms": "dev-master",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "predis/predis": "^1.1",
        "processmaker/nayra": "dev-master",
        "subcep/firmas": "dev-master"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "JDD\\FileBrowser\\": "packages/jdd/FileBrowser/src"
        },
        "files": ["app/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "require-plugin": [
            "@composer require",
            "@php artisan require-plugin"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
